<!doctype html>
<html lang="pl">
    <head>
        <title>Interactive Wind Tunnel::{%block title %}Main{%endblock%}</title>
        <link rel="stylesheet" href="/assets/css/style.css" />
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    </head>
    <body>
    <div class="container">
      <!-- Static navbar -->
      <nav class="navbar navbar-default">
        <div class="container-fluid">
          <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
              <span class="sr-only">Toggle navigation</span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#">IWT|ITA</a>
          </div>
          <div id="navbar" class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
              <li class="{% if app.request.attributes.get('_route') == 'app_frontend_demo' %}active{% endif %}"><a href="{{path('app_frontend_demo')}}">Home</a></li>
              <li class="{% if app.request.attributes.get('_route') == 'app_frontend_about' %}active{% endif %}"><a href="{{path('app_frontend_about')}}">About</a></li>
            </ul>
          </div><!--/.nav-collapse -->
        </div><!--/.container-fluid -->
      </nav>
      <!-- Main component for a primary marketing message or call to action -->
      {% block content %}
      {% endblock %}
    </div> <!-- /container -->

        <script src="/jspm_packages/system.js"></script>
        <script src="/config.js"></script>
        <script>
            System.import("jquery");
            System.import("jquery-ui");
        </script>
        <script>
          (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
          })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

          ga('create', 'UA-65333773-1', 'auto');
          ga('send', 'pageview');

        </script>
        {% block javascript %}
        {% endblock %}
    </body>
</html>
